old =  csvread('Rotsee_Output_old/N2_out.dat');
old = old(:,2:end)
new = csvread('Rotsee_Output/NN_out.dat');
new = new(:,2:end)

diff = (new-old)
plot(diff(1:100,:))
hold on 
plot(diff(:,[1,2,3,end-2,end-1,end])-diff(:,[1,2,3,end-2,end-1,end]),'ro')


for i=1:100
hold off
plot(diff(100,:))
hold on 
    plot(diff(i,:),'r')
drawnow
end 